plot 1 w l lc rgb 'black' notitle,\
     "30_70_Helium.data" us ($2):($3) every ev title "He (y_i=0.95)" with li lt 1,\
     "30_70_CO2.data" us ($2):($3) every ev title "CO_2 (y_i=0.015)" with li lt 2,\
     "30_70_CH4.data" us ($2):($3) every ev title "CH_4 (y_i=0.035)" with li lt 3,\
     "co2_30_70_TAMOF-1_experiments.txt" w p ps 0.5 pt 7 lc rgb 'light-green' notitle,\
