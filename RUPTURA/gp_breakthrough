set origin 0.3333,0
plot 1 w l lc rgb 'black' notitle,\
     "50_50_Helium.data" us ($2):($3) every ev title "He (y_i=0.95)" with li lt 1,\
     "50_50_CO2.data" us ($2):($3) every ev title "CO_2 (y_i=0.025)" with li lt 2,\
     "50_50_CH4.data" us ($2):($3) every ev title "CH_4 (y_i=0.025)" with li lt 3,\
     "co2_50_50_TAMOF-1_experiments.txt" w p ps 0.5 pt 7 lc rgb 'light-green' notitle,\
     "ch4_50_50_TAMOF-1_experiments.txt" w p ps 0.5 pt 7 lc rgb 'light-blue' notitle

set origin 0.6666,0
plot 1 w l lc rgb 'black' notitle,\
     "70_30_Helium.data" us ($2):($3) every ev title "He (y_i=0.95)" with li lt 1,\
     "70_30_CO2.data" us ($2):($3) every ev title "CO_2 (y_i=0.035)" with li lt 2,\
